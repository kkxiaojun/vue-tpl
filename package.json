{
  "scripts": {
    "dev": "cross-env NODE_ENV='development' PROCESS_ENV='LOCAL' i61-webpack run dev",
    "build-prod": "cross-env NODE_ENV='production' PROCESS_ENV='PROD' i61-webpack run build",
    "build-dev": "cross-env NODE_ENV='production' PROCESS_ENV='DEV' i61-webpack run build",
    "build-test": "cross-env NODE_ENV='production' PROCESS_ENV='TEST' i61-webpack run build",
    "build-grey": "cross-env NODE_ENV='production' PROCESS_ENV='GREY' i61-webpack run build",
    "build-pre": "cross-env NODE_ENV='production' PROCESS_ENV='PRE' i61-webpack run build",
    "commitAll": "git add . && git-cz ",
    "commit": "git-cz ",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.vue\" \"src/**/*.jsx\"",
    "eslint": "eslint --fix --ext .vue,.js src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts,tsx,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
